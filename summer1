import java.util.Scanner;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner sc = new Scanner(System.in);
	    int number=0;
	    int sum=0;
	    do
	    {
	    System.out.println("Please enter the number");
	    number= sc.nextInt();
	    sum  = sum+number;
	        
	    }
	    while(sum < 100);
	    System.out.println("Done");
	}
}
